void SolveOne(int test_id, IStream &in, OStream &out) {
  int N;
  in >> N;
  int people = 2;
  for(int i = 0; i < N; i++) {
    Str name;
    in >> name;
    people++;
    if(name.size() >= 4 && name.substr(name.size() - 4, 4) == "+one") {
      people++;
    }
  }
  people += people == 13;
  out << people * 100;
}

void SolveMulti(IStream &in, OStream &out) {
  //std::ifstream input("in");
  int num_of_input = 1;
  //in >> num_of_input;
  for (int i = 0; i < num_of_input; i++) {
    //SolveOne(i + 1, input, out);
	SolveOne(i + 1, in, out);
  }
}